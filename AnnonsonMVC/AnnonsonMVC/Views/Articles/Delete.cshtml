@model AnnonsonMVC.ViewModels.ArticelViewModel

    @{
        ViewData["Title"] = "Delete";
    }
     <div class="row">
        <div class="col-md-9">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">Ändra kontouppgifter</h3>
                </div>
                <div class="panel-body">
                    <div class="form form-horizontal" role="form">
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Användarnamn</label>
                            <div class="col-sm-9">
                                <TextBox placeholder="Användarnamn"></TextBox>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Förnamn</label>
                            <div class="col-sm-9">
                                <TextBox placeholder="Förnamn"></TextBox>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Efternamn</label>
                            <div class="col-sm-9">
                                <TextBox placeholder="Efternamn"></TextBox>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="panel-footer">
                    <div class="row">
                        <div class="col-sm-9 col-sm-offset-3">
                            <Button ID="btnSave" runat="server" Text="Spara" CssClass="btn btn-primary" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
 
